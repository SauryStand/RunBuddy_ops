		values (#{username,jdbcType=VARCHAR}, #{password,jdbcType=VARCHAR})
	</insert>

	<update id="updateByPrimaryKey" parameterType="com.dw.model.User">
		update
		BP_TESTING_ACCOUNT
		set
		ACCOUNTNAME = 
		#{username,jdbcType=VARCHAR}
		where ACCOUNTNAME = #{username,jdbcType=VARCHAR}
	</update>

	<select id="getAll" resultMap="BaseResultMap">
