<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dw.dao.UserMapper">
	<resultMap id="BaseResultMap" type="com.dw.model.User">
		<id column="ACCOUNTNAME" property="username" javaType="string" jdbcType="VARCHAR" />

		<result column="ACCOUNTNAME" property="username" javaType="string" jdbcType="VARCHAR" />
		<result column="ACCOUNTPASSWD" property="password" javaType="string" jdbcType="VARCHAR" />
		
	</resultMap>
	<sql id="Base_Column_List">
